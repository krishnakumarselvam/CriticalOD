function [CurrTrialPoint] = OptimizeMetamodel(Betas,baseODMatrix,x_init)
    
%Here I optimize the quadratic metamodel with the constraints that the
%demand for each OD pair is >=0

RHS = baseODMatrix(:,3);


function [z] = obj(x0)
    z=0;
    z = z+ Betas*x0;
end
A   = ones(1,size(baseODMatrix,1));
lb  = zero
CurrTrialPoint = fmincon(@obj,x_init',A,NUM_VEHICLES_TO_REMOVE,[],[],[0,0],[10,10],[],options);
CurrTrialPoint=CurrTrialPoint';
end